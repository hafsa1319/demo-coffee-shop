nline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid' : fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-column-reverse' : reverse}\">\n            <div class=\"map-wrapper col-12 col-lg-12\">\n                <div class=\"google-map\" mbr-map=\"googleMap\">\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-12\">\n                <div class=\"row justify-content-center\">\n                    <div class=\"col-12 col-sm-6 col-lg-3\" mbr-if=\"showPhone\">\n                        <div class=\"card_box\">\n                            <h4 class=\"card-title mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Call Us</h4>\n                            <div class=\"contact_content\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcons\">\n                                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-phone mobi-mbri\"></span>\n                                </div>\n                                <p class=\"card_text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card_text\">+1 234-567-8900</p>\n                            </div>\n                        </div>\n                    </div>\n                    \n                    <div class=\"col-12 col-sm-6 col-lg-3\" mbr-if=\"showLocation\">\n                        <div class=\"card_box\">\n                            <h4 class=\"card-title mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Our Location</h4>\n                            <div class=\"contact_content\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcons\">\n                                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-home mobi-mbri\"></span>\n                                </div>\n                                <p class=\"card_text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card_text\">350 5th Ave, New York, NY 10118</p>\n                            </div>\n                        </div>\n                    </div>\n                    \n                    <div class=\"col-12 col-sm-6 col-lg-3\" mbr-if=\"showMail\">\n                        <div class=\"card_box\">\n                            <h4 class=\"card-title mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Write Us</h4>\n                            <div class=\"contact_content\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcons\">\n                                    <span mbr-icon class=\"mbr-iconfont mobi-mbri-letter mobi-mbri\"></span>\n                                </div>\n                                <p class=\"card_text mb-0 mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".card_text\">info@hafsa.de</p>\n                            </div>\n                        </div>\n                    </div>\n                    \n                    <div class=\"col-12 col-sm-6 col-lg-3\" mbr-if=\"showSocial\">\n                        <div class=\"card_box\">\n                            <h4 class=\"card-title mb-0 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Follow Us</h4>\n                            <div class=\"contact_content\">\n                                <div class=\"iconfont-wrapper\" mbr-if=\"showIcons\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\"></span>\n       