          "_name": "menu3",
          "_sourceTheme": "bistrom5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"menu menu2\" group=\"Menu\" plugins=\"DropDown, TouchSwipe\" always-top global once=\"menu\" not-draggable position-absolute>\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" name=\"fullWidth\" title=\"Full Width\" checked>\n        <header>Top Widget</header>\n        <input type=\"checkbox\" title=\"Top Widget\" name=\"topWidget\" checked>\n        <input type=\"checkbox\" title=\"Image\" name=\"showWidgetImage\" condition=\"topWidget\" checked>\n        <input type=\"range\" title=\"Image Size\" inline name=\"widgetImageSize\" min=\"3\" max=\"8\" step=\"0.1\" value=\"6.6\" condition=\"showWidgetImage && topWidget\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showAddress\" condition=\"topWidget\" checked>\n        <input type=\"checkbox\" title=\"Button\" name=\"showButtons\" condition=\"topWidget\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\">\n        <input type=\"range\" title=\"Logo Size\" inline name=\"logoSize\" min=\"3\" max=\"8\" step=\"0.1\" value=\"4\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Brand Name\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Menu Items\" name=\"showItems\" condition=\"false\">\n        <header>Styles</header>\n        <input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" condition=\"!topWidget\">\n        <input type=\"checkbox\" title=\"Collapsed\" name=\"collapsed\" condition=\"false\">\n        <!-- <input type=\"checkbox\" title=\"Transparent\" name=\"transparent\" >\n        <input type=\"range\" title=\"Opacity\" name=\"opacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"transparent\"> -->\n        <input type=\"color\" title=\"Color Menu\" name=\"menuBgColor\" value=\"#fafafa\">\n        <input type=\"color\" title=\"Hover Dropdown Link\" name=\"hoverDropdownLink\" value=\"#e5673b\">\n        <input type=\"color\" title=\"Hamburger\" name=\"hamburgerColor\" value=\"#373c42\">\n        <header>Offcanvas</header>\n        <input type=\"checkbox\" title=\"Offcanvas\" name=\"showCanvas\" checked>\n        <input type=\"checkbox\" title=\"Image\" name=\"showImage\" condition=\"showCanvas\" checked>\n        <input type=\"range\" title=\"Image Size\" inline name=\"imageSize\" min=\"3\" max=\"8\" step=\"0.1\" value=\"5\" condition=\"showImage && showCanvas\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" condition=\"showCanvas\" checked>\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" condition=\"showCanvas\" checked>\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcons\" condition=\"showCanvas\" checked>\n        <select title=\"Amount\" name=\"iconsAmount\" condition=\"showIcons && showCanvas\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#4a4a4a\" condition=\"showIcons && showCanvas\">\n        <input type=\"color\" title=\"Offcanvas Color\" name=\"offcanvasColor\" value=\"#d8d8d8\" condition=\"showCanvas\">\n    </mbr-parameters>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\" mbr-if=\"topWidget\">\n        <div class=\"top_widget\">\n            <p class=\"widget_address mb-0 mbr-fonts-style\" mbr-if=\"showAddress\" data-app-selector=\".widget_address\" mbr-theme-style=\"display-4\"><span class=\"mobi-mbri mobi-mbri-map-pin mbr-iconfont\"></span><a href=\"#\" class=\"text-primary\">350 5th Ave, New York, NY 10118</a></p>\n            <div mbr-if=\"showWidgetImage\" class=\"widget_image\">\n                <img src=\"@PROJECT_PATH@/assets/images/hafsa-logo-standard-colour-2.svg\" alt mbr-style=\"{'heig